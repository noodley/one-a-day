<div class="row">
  <script type="text/javascript">
    function addItem(paper,level,entry) 
    {
      xCoord = 150;
      // We use a 20 space padding between each checkbox
      yCoord = level * 50 + (20 * (level - 1));
      size = 50;
      roundedRadius = 10;
      color = "#000"

      var rect = paper.rect(xCoord, yCoord, size, size, roundedRadius);
      rect.attr("stroke", color);

      var t = paper.text(240, yCoord + 25, entry);
      t.attr({font: "25px Fontin-Sans, Arial", fill: "#000", "text-anchor": "start"});
    }

    // Creates canvas 320 Ã— 200 at 10, 50
    var paper = Raphael(30, 40, 640, 480);

    addItem(paper,1,"First Entry");
    addItem(paper,2,"Second Entry");
    addItem(paper,3,"Third Entry");
    addItem(paper,4,"Fourth Entry");
    addItem(paper,5,"Fifth Entry");
    addItem(paper,6,"Sixth Entry");
  </script>
</div>
